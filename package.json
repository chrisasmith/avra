    "test": "CONFIG_ENV=test node node_modules/.bin/jest --colors --verbose false",
    "test:watch": "CONFIG_ENV=test node node_modules/.bin/jest --colors --watch --verbose false",
    "test:lint": "node ./node_modules/eslint/bin/eslint.js ./ --ext .js --ignore-path .gitignore --cache",
    "deploy": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "watch": "webpack -w --progress --debug --output-pathinfo",
    "postinstall": "if [ $BUILD_WEBPACK_ASSETS ]; then npm run deploy; fi",
    "build:langs": "./node_modules/.bin/babel-node scripts/translate.js",
    "server": "CONFIG_ENV=beta webpack-dev-server --content-base ./dist/public/ --port 3030 --history-api-fallback",
    "server:local": "webpack-dev-server --content-base ./dist/public/ --port 3030 --history-api-fallback",
    "server:watch": "node start.js",
    "start": "node ./dist/server.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "bail": true,
    "setupFiles": [
      "./shim.js",
      "./enzyme.js"
    ],
    "roots": [
